{
  "name": "mic-babel-preset",
  "version": "0.0.0",
  "description": "The babel preset we use at Mic",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && mkdir -p dist && babel lib --out-dir dist",

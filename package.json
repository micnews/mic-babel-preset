  },
  "homepage": "https://github.com/micnews/mic-babel-preset#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-tape-runner": "^2.0.1",
    "miclint": "^2.0.0"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "fast-async": "^6.1.2"
